<!DOCTYPE html>
<meta charset="utf-8">
<title>Redirecting to {{ url }}</title>
<meta name="description" content="{{ description }}" />
<meta http-equiv="refresh" content="0; URL={{ url }}">
<link rel="canonical" href="{{ url }}">
{{# google_analytics }}
<script type="text/javascript" id="ga-bootstrap" data-tracking-id="{{ google_analytics.tracking_id }}" data-redirect-url="{{ url }}" data-redirect-id="{{ id }}">
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

    var runningScript = document.getElementById('ga-bootstrap');
    var trackingId = runningScript.getAttribute('data-tracking-id');
    var redirectId = runningScript.getAttribute('data-redirect-id');
    var redirectUrl = runningScript.getAttribute('data-redirect-url');

    ga('create', trackingId, 'auto');
    ga('set', 'dimension1', redirectId);
    ga('set', 'dimension2', redirectUrl);
    ga('send', 'pageview');
</script>
{{/ google_analytics }}
<body>Redirecting to {{ url }} - <a href="{{ url }}">Click Here if this is taking too long.</a></body>